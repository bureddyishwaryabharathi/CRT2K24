#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,t,r,d,s=0;
    scanf("%d",&n);
    t=n;
    r=log10(n)+1;
    while(n>0)
    {
        d=n%10;
        n=n/10;
        s=s+pow(d,r);
        
    }
    if(s==t)
    {
        printf("Yes");
    }
    else
    {
        printf("No");
    }

       
    return 0;
}
