#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,rem,sum;
    scanf("%d",&n);
    sum=0;
    while(n>=1)
    {
        rem =n%10;
        n=n/10;
        if(rem%2!=0){
            sum=sum+rem;
        }
    }
        printf("%d",sum);
    
    
    
    

    
    

       
    return 0;
}
